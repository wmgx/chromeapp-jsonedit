{
	"manifest_version": 2,
	"name": "Json",
	"version": "1.1.1",
	"description": "简单Json编辑器11",
	"icons":
	{
		"16": "favicon.ico",
		"48": "favicon.ico",
		"128": "favicon.ico"
	},
	"permissions":
	[
		"storage", "clipboardRead"
	],
	"homepage_url": "https://github.com/wmgx",
    "app":
	{
		"launch": {
            "local_path": "index.html"
        }
	},
	"background": {
		"persistent": true,
		"page": "background.html"
	},
	
	"omnibox": { "keyword" : "json" },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
